<% if !(@user_mentee_application.current_status == "accepted") %>
  <%= button_to "Promote to #{button_text}",
 user_mentee_application_promotions_path(@user_mentee_application.id),
  method: :post,
  class:"btn btn-primary capitalize btn-link btn-xs sm:btn-sm hover:no-underline" 
%>
<% else %>
  "Accepted!!"
<% end %>

